<h1> 첫번째 페이지 </h1>

 <a href="#"><img src="/assets/메인페이지.PNG" /></a>
 
 
<% unless user_signed_in? %>
    <form action="/users/sign_in" method="POST">
        <input class="btn btn-default" type="submit" value="로그인">
    </form>

<br>

<form action="/users/sign_up" method="GET">
    <input class="btn btn-default" type="submit" value="회원가입">
</form>

<% else %>

<form action="/users/sign_out" method="GET">
        <input class="btn btn-default" type="submit" value="로그아웃">
</form>

<% end %>